{
  "name": "lmg-admin",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.0-11",
    "@fortawesome/free-regular-svg-icons": "^5.1.0-11",
    "@fortawesome/free-solid-svg-icons": "^5.1.0-11",
    "@fortawesome/react-fontawesome": "^0.1.0-11",
    "apollo-link-state": "0.4.1",
    "aws-amplify": "0.4.1",
    "aws-amplify-react": "0.1.38",
    "aws-appsync": "^1.0.23",
    "aws-appsync-react": "^1.0.14",
    "aws-sdk": "2.247.1",
    "bootstrap": "4.0.0",
    "classnames": "2.2.5",
    "graphql-tag": "2.9.2",
    "node-sass-chokidar": "1.2.2",
    "npm-run-all": "4.1.2",
    "react": "16.3.1",
    "react-apollo": "^2.1.4",
    "react-dom": "16.3.1",
    "react-router-dom": "4.2.2",
    "react-scripts": "1.1.4",
    "reactstrap": "6.0.1",
    "shortid": "2.2.8"
  },
  "scripts": {
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build-css": "node-sass-chokidar --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./node_modules src/ -o src/ --watch --recursive"
  },
  "devDependencies": {
    "prop-types": "latest"
  }
}
